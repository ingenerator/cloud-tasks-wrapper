{
    "name": "ingenerator/cloud-tasks-wrapper",
    "description": "Library for dispatching and processing Google Cloud Tasks from PHP",
    "homepage": "https://github.com/ingenerator/cloud-tasks-wrapper",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Andrew Coulton",
            "email": "andrew@ingenerator.com"
        }
    ],
    "require": {
        "php": "~8.1.0 || ~8.2.0",
        "ext-bcmath": "*",
        "google/cloud-tasks": "^1.8",
        "google/gax": "^1.29.1",
        "google/protobuf": "^3.22.0",
        "ingenerator/oidc-token-verifier": "^0.3 || ^1.0",
        "ingenerator/php-utils": "^1.13 || ^2.0",
        "psr/http-message": "^1.0 || ^2.0"
    },
    "require-dev": {
        "fig/log-test": "^1.1",
        "guzzlehttp/psr7": "^2.6",
        "phpunit/phpunit": "^9.5.5"
    },
    "repositories": [
        {"type": "composer", "url": "https://php-packages.ingenerator.com"}
    ],
    "support": {
        "source": "https://github.com/ingenerator/cloud-tasks-wrapper",
        "issues": "https://github.com/ingenerator/cloud-tasks-wrapper/issues"
    },
    "autoload": {
        "psr-4": {
            "Ingenerator\\CloudTasksWrapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "test\\mock\\Ingenerator\\CloudTasksWrapper\\": "test/mock",
            "test\\unit\\Ingenerator\\CloudTasksWrapper\\": "test/unit"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "ext-bcmath": "8"
        }
    }
}
